{%- assign first_block = section.blocks | first -%}
<style>
  #shopify-section-{{ section.id }} {
    --section-padding-top: {{ section.settings.padding_top }}px;
    --section-padding-bottom: {{ section.settings.padding_bottom }}px;
    {%- render 'section-variables', section: section -%}
    {%- if first_block != blank and first_block.settings.image != blank and section.settings.image_height == 'adapt' %}
      --ratio-percent: {{ 1 | divided_by: first_block.settings.image.aspect_ratio | times: 100 }}%;
    {% endif -%}
  }

  {%- assign first_block_image_mobile = first_block.settings.image_mobile | default: first_block.settings.image -%}
  {%- if first_block != blank and first_block_image_mobile != blank and section.settings.image_height_mobile == 'adapt' %}
    @media screen and (max-width: 767px) {
      #shopify-section-{{ section.id }} {
        --ratio-percent: {{ 1 | divided_by: first_block_image_mobile.aspect_ratio | times: 100 }}%;
      }
    }
  {% endif -%}
</style>

<div class="section section--padding{% if section.settings.divider %} section--divider{% endif %}{% if section.settings.rounded %} section--rounded relative{% endif %}">
  <div class="page-width{% if section.settings.full_width %} page-width--full{% endif %} relative">
    {%- liquid
      assign with_background = false
      if section.settings.gradient_background_2 != blank and section.settings.gradient_background_2 != 'rgba(0,0,0,0)'
        assign with_background = true
      endif
      if section.settings.color_background_2 != blank and section.settings.color_background_2 != 'rgba(0,0,0,0)'
        assign with_background = true
      endif

      assign with_gradient = false
      if section.settings.gradient_highlight != blank and section.settings.gradient_highlight != 'rgba(0,0,0,0)'
        assign with_gradient = true
      endif
    -%}
    <div class="grid{% if with_background %} gap-4{% else %} gap-10{% endif %} lg:hidden">
      {%- for block in section.blocks -%}
        <div class="image-with-text{% if with_background %} with-background{% endif %} with-scrolling flex flex-col flex-col-reverse overflow-hidden">
          <div class="image-with-text__item grow shrink-0 relative">
            <div class="image-with-text__media h-full">
              <media-element class="image-with-text__image block media--{{ section.settings.image_height }} mobile:media--{{ section.settings.image_height_mobile }} relative overflow-hidden">
                {%- case block.type -%}
                  {%- when 'image' -%}
                    <picture class="media media--height block w-full h-full relative overflow-hidden">
                      {%- if block.settings.image != blank -%}
                        {%- assign sizes = '(max-width: 767px) 100vw, 50vw' -%}
                        {%- if block.settings.image_mobile -%}
                          <source
                            media="(max-width: 767px)"
                            srcset="{{ block.settings.image_mobile | image_url: width: '400x' }} 400w, {{ block.settings.image_mobile | image_url: width: '600x' }} 600w, {{ block.settings.image_mobile | image_url: width: '800x' }} 800w, {{ block.settings.image_mobile | image_url: width: '1000x' }} 1000w"
                            width="{{ block.settings.image_mobile.width }}"
                            height="{{ block.settings.image_mobile.height }}"
                          >
                        {%- endif -%}
          
                        {{- block.settings.image | image_url: width: block.settings.image.width | image_tag: loading: 'lazy', sizes: sizes, widths: '300,400,500,600,700,800,900,1000,1200,1400,1600', is: 'lazy-image' -}}
                      {%- else -%}
                        {{- 'lifestyle-1' | placeholder_svg_tag: 'placeholder' -}}
                      {%- endif -%}
                    </picture>

                  {%- when 'video' -%}
                    <div class="media media--height block w-full h-full relative overflow-hidden">
                      {%- render 'video',
                        sizes: sizes,
                        loading: 'lazy',
                        fetchpriority: 'low',
                        video: block.settings.video,
                        video_url: block.settings.video_url,
                        muted: false,
                        autoplay: false,
                        loop: true,
                        description: block.settings.description,
                        cover_image: block.settings.cover_image,
                        show_play_button: true
                      -%}
                    </div>
                {%- endcase -%}
              </media-element>
            </div>
          </div>
    
          <div class="image-with-text__item grow shrink-0 {{ section.settings.content_position }}">
            <div class="rich-text lg:text-{{ section.settings.text_alignment }} text-{{ section.settings.text_alignment_mobile }} relative z-1">
              {%- if block.settings.icon_image != blank -%}
                <div class="leading-none">
                  <figure class="media media--transparent relative inline-block">
                    {%- capture sizes -%}{{ block.settings.icon_width }}px{%- endcapture -%}
                    {%- capture widths -%}{{ block.settings.icon_width }}, {{ block.settings.icon_width | times: 2 | at_most: block.settings.icon_image.width }}{%- endcapture -%}
                    {{- block.settings.icon_image | image_url: width: block.settings.icon_image.width | image_tag: loading: 'lazy', sizes: sizes, widths: widths, class: 'image-fit' -}}
                  </figure>
                </div>
              {%- elsif block.settings.icon != 'none' -%}
                <p class="leading-none">
                  {%- render 'icon-guarantee', icon: block.settings.icon, size: 'lg', class: 'inline-block' -%}
                </p>
              {%- endif -%}

              {%- if block.settings.subheading != blank -%}
                {%- liquid
                  if block.settings.heading_size == 'title-sm'
                    assign subtext_size = 'subtext-md'
                  else
                    assign subtext_size = 'subtext-lg'
                  endif
                -%}
                <p class="heading uppercase leading-none tracking-widest {{ subtext_size }}">
                  {{- block.settings.subheading -}}
                </p>
              {%- endif -%}

              {%- if block.settings.heading != blank -%}
                <{{ block.settings.heading_tag }} class="heading leading-none {{ block.settings.heading_size }}">
                  {%- if block.settings.highlighted_text == 'none' -%}
                    <split-words class="split-words block" data-animate="fade-up-large">{{ block.settings.heading }}</split-words>
                  {%- else -%}
                    {%- render 'highlighted-text', content: block.settings.heading, style: block.settings.highlighted_text, scribble: block.settings.highlighted_scribble, gradient: with_gradient -%}
                  {%- endif -%}
                </{{ block.settings.heading_tag }}>
              {%- endif -%}

              {%- if block.settings.description != blank -%}
                {%- liquid
                  if block.settings.heading_size == 'title-sm' or block.settings.heading_size == 'title-md'
                    assign subtext_size = 'subtext-md'
                  else
                    assign subtext_size = 'subtext-lg'
                  endif
                -%}
                <div class="rte leading-normal {{ subtext_size }}">
                  {%- render 'highlighted-text', content: block.settings.description, style: block.settings.highlighted_text, scribble: block.settings.highlighted_scribble, gradient: with_gradient -%}
                </div>
              {%- endif -%}

              {%- if block.settings.button_label != blank -%}
                {%- render 'button',
                  label: block.settings.button_label,
                  link: block.settings.button_link,
                  style: block.settings.button_style,
                  external: block.settings.button_external,
                  show_icon: block.settings.button_icon
                -%}
              {%- endif -%}
            </div>
          </div>
        </div>
      {%- endfor -%}
    </div>
    <scrolling-banner class="scrolling-banner hidden lg:block">
      <sticky-element class="md:sticky top-0 image-with-text{% if with_background %} with-background{% endif %} with-scrolling flex lg:gap-2 flex-col{% if section.settings.layout == 'image_first' %} lg:flex-row{% else %} lg:flex-row-reverse{% endif %} overflow-hidden">
        <div class="image-with-text__item grow shrink-0{% if section.settings.image_width == 'small' %} lg:grow-0{% endif %} relative">
          <div class="image-with-text__media h-full">
            <media-element class="image-with-text__image block media--{{ section.settings.image_height }} mobile:media--{{ section.settings.image_height_mobile }} relative overflow-hidden">
              {%- for block in section.blocks -%}
                {%- case block.type -%}
                  {%- when 'image' -%}
                    <picture class="media media--height block w-full h-full{% unless forloop.first %} absolute top-0 left-0{% else %} relative{% endunless %} overflow-hidden" aria-controls="shopify-block-{{ block.id }}">
                      {%- if block.settings.image != blank -%}
                        {%- assign sizes = '(max-width: 767px) 100vw, 50vw' -%}
                        {%- if block.settings.image_mobile -%}
                          <source
                            media="(max-width: 767px)"
                            srcset="{{ block.settings.image_mobile | image_url: width: '400x' }} 400w, {{ block.settings.image_mobile | image_url: width: '600x' }} 600w, {{ block.settings.image_mobile | image_url: width: '800x' }} 800w, {{ block.settings.image_mobile | image_url: width: '1000x' }} 1000w"
                            width="{{ block.settings.image_mobile.width }}"
                            height="{{ block.settings.image_mobile.height }}"
                          >
                        {%- endif -%}
          
                        {{- block.settings.image | image_url: width: block.settings.image.width | image_tag: loading: 'lazy', sizes: sizes, widths: '300,400,500,600,700,800,900,1000,1200,1400,1600', is: 'lazy-image' -}}
                      {%- else -%}
                        {{- 'lifestyle-1' | placeholder_svg_tag: 'placeholder' -}}
                      {%- endif -%}
                    </picture>

                  {%- when 'video' -%}
                    <div class="media media--height block w-full h-full{% unless forloop.first %} absolute top-0 left-0{% else %} relative{% endunless %} overflow-hidden" aria-controls="shopify-block-{{ block.id }}">
                      {%- render 'video',
                        sizes: sizes,
                        loading: 'lazy',
                        fetchpriority: 'low',
                        video: block.settings.video,
                        video_url: block.settings.video_url,
                        muted: true,
                        autoplay: true,
                        loop: true,
                        description: block.settings.description,
                        cover_image: block.settings.cover_image,
                        show_play_button: false
                      -%}
                    </div>

                {%- endcase -%}
              {%- else -%}
                <div class="media media--height block w-full h-full{% unless forloop.first %} absolute top-0 left-0{% else %} relative{% endunless %} overflow-hidden">
                  {{- 'lifestyle-1' | placeholder_svg_tag: 'placeholder' -}}
                </div>
              {%- endfor -%}
            </media-element>
          </div>
        </div>

        <div class="image-with-text__item grow shrink-0{% if section.settings.image_width == 'large' %} lg:grow-0{% endif %} relative">
          {%- for block in section.blocks -%}
            <div class="image-with-text__content flex {{ section.settings.content_position }} w-full h-full{% unless forloop.first %} absolute top-0 left-0 opacity-0 pointer-events-none{%  else %} relative{% endunless %}" id="shopify-block-{{ block.id }}" style="z-index:{{ forloop.index }}">
              <div class="rich-text lg:text-{{ section.settings.text_alignment }} text-{{ section.settings.text_alignment_mobile }} relative z-1">
                {%- if block.settings.icon_image != blank -%}
                  <div class="leading-none">
                    <figure class="media media--transparent relative inline-block">
                      {%- capture sizes -%}{{ block.settings.icon_width }}px{%- endcapture -%}
                      {%- capture widths -%}{{ block.settings.icon_width }}, {{ block.settings.icon_width | times: 2 | at_most: block.settings.icon_image.width }}{%- endcapture -%}
                      {{- block.settings.icon_image | image_url: width: block.settings.icon_image.width | image_tag: loading: 'lazy', sizes: sizes, widths: widths, class: 'image-fit' -}}
                    </figure>
                  </div>
                {%- elsif block.settings.icon != 'none' -%}
                  <p class="leading-none">
                    {%- render 'icon-guarantee', icon: block.settings.icon, size: 'lg', class: 'inline-block' -%}
                  </p>
                {%- endif -%}
  
                {%- if block.settings.subheading != blank -%}
                  {%- liquid
                    if block.settings.heading_size == 'title-sm'
                      assign subtext_size = 'subtext-md'
                    else
                      assign subtext_size = 'subtext-lg'
                    endif
                  -%}
                  <p class="heading uppercase leading-none tracking-widest {{ subtext_size }}">
                    {{- block.settings.subheading -}}
                  </p>
                {%- endif -%}
  
                {%- if block.settings.heading != blank -%}
                  <{{ block.settings.heading_tag }} class="heading leading-none {{ block.settings.heading_size }}">
                    {%- if block.settings.highlighted_text == 'none' -%}
                      <split-words class="split-words block" data-animate="fade-up-large">{{ block.settings.heading }}</split-words>
                    {%- else -%}
                      {%- render 'highlighted-text', content: block.settings.heading, style: block.settings.highlighted_text, scribble: block.settings.highlighted_scribble, gradient: with_gradient -%}
                    {%- endif -%}
                  </{{ block.settings.heading_tag }}>
                {%- endif -%}
  
                {%- if block.settings.description != blank -%}
                  {%- liquid
                    if block.settings.heading_size == 'title-sm' or block.settings.heading_size == 'title-md'
                      assign subtext_size = 'subtext-md'
                    else
                      assign subtext_size = 'subtext-lg'
                    endif
                  -%}
                  <div class="rte leading-normal {{ subtext_size }}">
                    {%- render 'highlighted-text', content: block.settings.description, style: block.settings.highlighted_text, scribble: block.settings.highlighted_scribble, gradient: with_gradient -%}
                  </div>
                {%- endif -%}
  
                {%- if block.settings.button_label != blank -%}
                  {%- render 'button',
                    label: block.settings.button_label,
                    link: block.settings.button_link,
                    style: block.settings.button_style,
                    external: block.settings.button_external,
                    show_icon: block.settings.button_icon
                  -%}
                {%- endif -%}
              </div>
            </div>
          {%- endfor -%}
        </div>
      </sticky-element>
    </scrolling-banner>
  </div>
</div>

